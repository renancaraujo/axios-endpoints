{
    "env": {
        "dev": {
            "presets": [
                [
                    "@babel/preset-env",
                    {
                        "modules": false
                    }
                ]
            ],
            "plugins": [
                "@babel/plugin-proposal-object-rest-spread",
                "@babel/plugin-external-helpers",
                "@babel/plugin-proposal-class-properties"
            ]
        },
        "test": {
            "presets": [
                [
                    "@babel/preset-env"
                ]
            ],
            "plugins": [
                "@babel/plugin-proposal-class-properties"
            ]
        }
    }
}
