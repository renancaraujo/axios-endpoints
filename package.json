{
  "name": "axios-endpoints",
  "version": "0.0.2",
  "description": "Axios endpoints helps you to create mor concise endpoint mapping with axios. ",
  "main": "dist/umd/index.js",
  "scripts": {
    "build": "BABEL_ENV=dev rollup -c",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "lint": "eslint --ext=js --ext=jsx ./src",
    "lint:fix": "eslint --fix --ext=js --ext=jsx ./src",
    "precommit": "lint-staged"
  },
  "author": "Renan C. Ara√∫jo",
  "license": "MIT",
  "homepage": "https://github.com/renancaraujo/axios-endpoints#readme",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:renancaraujo/axios-endpoints.git"
  },
  "bugs": {
    "url": "https://github.com/renancaraujo/axios-endpoints/issues"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json}": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "peerDepencies": {
    "axios": "^0.18.0"
  },
  "devDependencies": {
    "axios": "^0.18.0",
    "axios-mock-adapter": "^1.15.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^23.0.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "^0.14.3",
    "jest": "^23.1.0",
    "lint-staged": "^7.2.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.13.5",
    "regenerator-runtime": "^0.11.1",
    "rollup": "^0.60.7",
    "rollup-plugin-babel": "^3.0.4"
  }
}
